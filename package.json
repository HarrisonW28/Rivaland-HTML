{
  "name": "rivaland-html",
  "version": "1.0.0",
  "description": "Rivaland HTML project with modular SCSS",
  "scripts": {
    "sass": "sass scss/style.scss style.css",
    "sass:watch": "sass --watch scss/style.scss:style.css",
    "sass:prod": "sass scss/style.scss style.css --style compressed --no-source-map",
    "build": "npm run sass:prod"
